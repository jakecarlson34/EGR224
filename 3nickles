`timescale 1ns / 1ps
//////////////////////////////////////////////////////////////////////////////////
// Author: Jake Carlson
// Scenario: Nickle, Nickle, Nickle
//////////////////////////////////////////////////////////////////////////////////


module vendingMachine_tb();

vendingMachine f1 (clk, R, C, D, N);

reg clk, D, N;
wire C, R;
initial begin
D = 0;
N = 0;
clk = 0;
end

initial begin 
#40 N = 1;
#20 N = 0;
#40 N = 1;
#20 N = 0;
#40 N = 1;
#20 N = 0;
end

always #10 clk = ~clk;

initial #300 $finish;

endmodule
